<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <theme>standard</theme>  <!-- standard, dark -->
    
        <!-- document encoding -->
        <encoding>UTF-8</encoding>  <!-- defaults to UTF-8 -->
        
        <!-- protocol for script and stylesheets -->       
        <protocol>https</protocol>  <!-- defaults to https -->
        
        <!-- title of the document -->
        <documentTitle>Cogmento Reports</documentTitle>
        
        <!-- report name - displayed at top-nav -->
        <reportName>Automation Report for</reportName>
        
        <!-- report headline - displayed at top-nav, after reportHeadline -->
        <reportHeadline>POM Framework</reportHeadline>
        
        <!-- global date format override -->
        <!-- defaults to yyyy-MM-dd -->
        <dateFormat>yyyy-MM-dd</dateFormat>
        
        <!-- global time format override -->
        <timeFormat>HH:mm:ss</timeFormat>  <!-- defaults to HH:mm:ss -->
        
        <!-- custom javascript -->
        <scripts>
            <![CDATA[
                $(document).ready(function() {
                    
                });
            ]]>
        </scripts>
        
        <!-- custom styles -->
        <styles>
            <![CDATA[
                
            ]]>
        </styles>
    </configuration>
</extentreports>